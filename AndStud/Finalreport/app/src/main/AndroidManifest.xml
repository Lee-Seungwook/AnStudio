<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cookandroid.finalreport">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_fitness"
        android:label="헬스 도우미"
        android:roundIcon="@mipmap/ic_launcher_fitness_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:configChanges="orientation|screenSize">


       <activity android:name=".SubActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".ChestActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".BackActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".LegActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".FoodDbActivity"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>